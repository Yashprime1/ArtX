<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>

  <div class="row">
    <div class="col s2 m3 l3"></div>
    <div class="col s8 m6 l6">
        <h5 class="center-align"> Recently Added &#x1f525; </h5>
    </div>
    <div class="col s2 m3 l3"></div>
</div>    

<div class="row">
  <% for(var i=0; i<art_list.length; i++) { %>
  <div class="col s12 m6 l6">
    <img class="materialboxed responsive-img"  style="width: 200px;height: 200px; padding: 30px;margin-left: auto;margin-right: auto;display: block;" src="<%= art_list[i]['arturl'] %>" >
    <p class="center-align"> <%= art_list[i]['artname'] %> by <span style="color:red;"><%= art_list[i]['artistname'] %></span></p>
  </div>
  <% } %>

 
  </div>   
</main>

<footer class="page-footer">
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>