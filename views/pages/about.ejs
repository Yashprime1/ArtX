<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>


<div class="row">
    <div class="col s12 m2 l2"></div>
       <div class="col s12 m8 l8">
        <div class="card medium">
          <div class="card-image">
            <img class="materialboxed" width="650" src="/images/6.jpg" >
            
          </div>
          <div class="card-content">
            <p>About</p>
          </div>
          <div class="card-action">
             <a class="btn-floating btn-large waves-effect waves-light red"><i class="material-icons">favorite</i></a>
          </div>
        </div>
      </div>
      <div class="col s12 m2 l2"></div>
</div>

<div class="row">
    <form class="col s12 m12 l12">
      <div class="row">
        <div class="input-field col s12 m6 l6">
          <i class="material-icons prefix">app_registration</i>
          <input id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Name of Art</label>
        </div>
        <div class="input-field col s12 m6 l6">
          <i class="material-icons prefix">phone</i>
          <input id="icon_telephone" type="tel" class="validate">
          <label for="icon_telephone">Telephone</label>
        </div>
      </div>
    </form>
  </div>

<div class="row">
    <div class="col s12 m12 l12">
        
    </div>
</div>

</main>

<footer class="page-footer">
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>