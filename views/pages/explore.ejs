<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>


<div class="row">
    <div class="col s12 m12 l12">
      <div class="carousel ">
        <% for(var i=0; i<art_list.length; i++) { %>
          <div class="carousel-item hoverable" href="#one!">
              <img class="materialboxed responsive-img " style="width: 200px;height: 200px" src="<%= art_list[i]['arturl'] %>" >
              <p class="center-align"> <%= art_list[i]['artname'] %> by <span style="color:red;"><%= art_list[i]['artistname'] %></span></p>
          </div>
        <% } %>
        </div>
              
    </div>
</div>

</main>

<footer class="page-footer">
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>